{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "generalTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "labelText": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "label",
        "label": "Label"
      },
      "link": {
        "sling:resourceType": "wcm/dialogs/components/pathpicker",
        "rootPath": "/content",
        "name": "link",
        "label": "Link"
      },
      "openInNewTab": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "openInNewTab",
        "label": "Open link in a new tab"
      }
    },
    "iconTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Icon",
      "showLeftIcon": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "showIconLeft",
        "label": "Show left icon?"
      },
      "iconLeftContainer": {
        "sling:resourceType": "wcm/dialogs/components/container",
        "iconLibType": {
          "sling:resourceType": "wcm/dialogs/components/select",
          "name": "leftIcon/iconLibType",
          "label": "Left Icon Library",
          "required": true,
          "datasource": {
            "sling:resourceType": "/apps/iconLibraryDatasource"
          }
        },
        "selectOrTypeLeft": {
          "sling:resourceType": "wcm/dialogs/components/radio",
          "name": "leftIcon/selectOrType",
          "label": "Select icon or type it",
          "required": true,
          "select": {
            "sling:resourceType": "wcm/dialogs/components/radio/option",
            "label": "Select it",
            "value": "select",
            "selected": true
          },
          "type": {
            "sling:resourceType": "wcm/dialogs/components/radio/option",
            "label": "Type it",
            "value": "type"
          }
        },
        "typedLeft": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "iconLeft": {
            "sling:resourceType": "wcm/dialogs/components/textfield",
            "name": "leftIcon/icon",
            "label": "Name of the left icon"
          },
          "ws:display": {
            "condition": {
              "sourceName": "leftIcon/selectOrType",
              "values": "type"
            }
          }
        },
        "iconFALeft": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "selectableIcon": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/icons/fontawesome",
              "name": "leftIcon/icon",
              "label": "Left icon"
            },
            "ws:display": {
              "condition": {
                "sourceName": "leftIcon/selectOrType",
                "values": "select"
              }
            }
          },
          "iconSizeLeft": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/iconsize/fontawesome",
              "name": "leftIcon/iconSize",
              "label": "Left icon size"
            }
          },
          "ws:display": {
            "condition": {
              "sourceName": "leftIcon/iconLibType",
              "values": "fas"
            }
          }
        },
        "iconMDLeft": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "selectableIcon": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/icons/materialdesign",
              "name": "leftIcon/icon",
              "label": "Left icon"
            },
            "ws:display": {
              "condition": {
                "sourceName": "leftIcon/selectOrType",
                "values": "select"
              }
            }
          },
          "iconSize": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/iconsize/materialdesign",
              "name": "leftIcon/iconSize",
              "label": "Left icon size"
            }
          },
          "ws:display": {
            "condition": {
              "sourceName": "leftIcon/iconLibType",
              "values": "mdi"
            }
          }
        },
        "ws:display": {
          "condition": {
            "sourceName": "showIconLeft",
            "values": "true"
          }
        }
      },
      "showRightIcon": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "showIconRight",
        "label": "Show right icon?"
      },
      "iconRightContainer": {
        "sling:resourceType": "wcm/dialogs/components/container",
        "iconLibType": {
          "sling:resourceType": "wcm/dialogs/components/select",
          "name": "rightIcon/iconLibType",
          "label": "Right Icon Library",
          "required": true,
          "datasource": {
            "sling:resourceType": "/apps/iconLibraryDatasource"
          }
        },
        "selectOrTypeRight": {
          "sling:resourceType": "wcm/dialogs/components/radio",
          "name": "rightIcon/selectOrType",
          "label": "Select icon or type it",
          "required": true,
          "select": {
            "sling:resourceType": "wcm/dialogs/components/radio/option",
            "label": "Select it",
            "value": "select",
            "selected": true
          },
          "type": {
            "sling:resourceType": "wcm/dialogs/components/radio/option",
            "label": "Type it",
            "value": "type"
          }
        },
        "typedRight": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "iconRight": {
            "sling:resourceType": "wcm/dialogs/components/textfield",
            "name": "rightIcon/icon",
            "label": "Name of the right icon"
          },
          "ws:display": {
            "condition": {
              "sourceName": "rightIcon/selectOrType",
              "values": "type"
            }
          }
        },
        "iconFARight": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "selectableIcon": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/icons/fontawesome",
              "name": "rightIcon/icon",
              "label": "Right icon"
            },
            "ws:display": {
              "condition": {
                "sourceName": "rightIcon/selectOrType",
                "values": "select"
              }
            }
          },
          "iconSizeRight": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/iconsize/fontawesome",
              "name": "rightIcon/iconSize",
              "label": "Right icon size"
            }
          },
          "ws:display": {
            "condition": {
              "sourceName": "rightIcon/iconLibType",
              "values": "fas"
            }
          }
        },
        "iconMDRight": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "selectableIcon": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/icons/materialdesign",
              "name": "rightIcon/icon",
              "label": "Right icon"
            },
            "ws:display": {
              "condition": {
                "sourceName": "rightIcon/selectOrType",
                "values": "select"
              }
            }
          },
          "iconSizeRight": {
            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "kyanite/components/common/icon/iconsize/materialdesign",
              "name": "rightIcon/iconSize",
              "label": "Right icon size"
            }
          },
          "ws:display": {
            "condition": {
              "sourceName": "rightIcon/iconLibType",
              "values": "mdi"
            }
          }
        },
        "ws:display": {
          "condition": {
            "sourceName": "showIconRight",
            "values": "true"
          }
        }
      }
    },
    "styleTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Style",
      "size": {
        "sling:resourceType": "wcm/dialogs/components/include",
        "path": "kyanite/components/link/attributes/size"
      },
      "variant": {
        "sling:resourceType": "wcm/dialogs/components/include",
        "path": "kyanite/components/link/attributes/variant"
      }
    }
  }
}