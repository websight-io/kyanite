{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "generalTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "sizeInclude": {
        "sling:resourceType": "wcm/dialogs/components/include",
        "include": {
          "sling:resourceSuperType": "/libs/kyanite/components/common/size",
          "normal": {
            "sling:hideResource": true
          }
        }
      },
      "alignmentInclude": {
        "sling:resourceType": "wcm/dialogs/components/include",
        "include": {
          "sling:resourceSuperType": "/libs/kyanite/components/common/alignment",
          "left": {
            "value": ""
          }
        }
      },
      "contentId": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "contentId",
        "label": "Content id"
      },
      "autoSwitch": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "autoSwitch",
        "label": "Auto switching"
      },
      "switchingTime": {
        "sling:resourceType": "wcm/dialogs/components/numberfield",
        "name": "switchingTime",
        "label": "Switching time",
        "min": 1,
        "max": 999,
        "ws:display": {
          "condition": {
            "sourceName": "autoSwitch",
            "values": true
          }
        }
      }
    },
    "modifiers": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Modifiers",
      "boxed": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "boxed",
        "label": "Borders"
      },
      "fullWidth": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "fullWidth",
        "label": "Full width"
      },
      "toggle": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "toggle",
        "label": "Toggle"
      },
      "toggleRounded": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "toggleRounded",
        "label": "Rounded",
        "ws:display": {
          "condition": {
            "sourceName": "toggle",
            "values": true
          }
        }
      }
    }
  }
}