            "sling:resourceType": "wcm/dialogs/components/include",
            "include": {
              "sling:resourceSuperType": "/apps/bulma/components/columns/attributes/evennumber",
              "name": "offset",
              "label": "Offset size",
              "sling:hideChildren": ["twelve"]
