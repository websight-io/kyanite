<?xml version="1.0" encoding="UTF-8"?>
<jcr:root   xmlns:jcr="http://www.jcp.org/jcr/1.0"
            xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
            xmlns:ws="http://ds.pl/websight"
            xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="ws:Page">
    <jcr:content
            jcr:primaryType="ws:PageContent"
            jcr:title="Table of content Hierarchy"
            sling:resourceType="kyanite/blogs/components/blogarticlepage"
            ws:lastModifiedBy="wsadmin"
            ws:template="/libs/kyanite/blogs/templates/blogarticlepage"
            navbarFixed="true"
            navbarFixedPosition="has-navbar-fixed-top"
            title="Article">
        <author
                jcr:primaryType="nt:unstructured"
                authorInfoSource="ownProperties"
                authorName="John Doe"
                authorRole="CEO"/>
        <pagecontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kyanite/common/components/pagecontainer">
            <navbarreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/navbarreference"/>
            <section
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/section/fixedsection"
                    renderAsHero="false">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kyanite/common/components/container/fixedcontainer">
                    <columns1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/columns/fixedcolumns"
                            columnsActivationLevel="is-desktop"
                            isMultiline="true">
                        <column1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="two"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <backbutton
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogpostbackbutton"/>
                        </column1>
                        <column2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogarticleheader
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogarticleheader">
                                <author
                                        jcr:primaryType="nt:unstructured"
                                        authorInfoSource="parentPage"/>
                            </blogarticleheader>
                        </column2>
                        <column3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    size="two"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogposttableofcontents
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogposttableofcontents"
                                    maxHeadingLevel="2"
                                    minHeadingLevel="6"
                                    showBackToTopButton="true"
                                    title="ToC"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                        </column3>
                        <column4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offset="two"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <title_1_3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h4"
                                    size="is-6"
                                    text="&lt;p>4444 wrong element (should be skipped)&lt;/p>&lt;p>&lt;/p>"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h2"
                                    size="is-4"
                                    text="&lt;p>22 with wrong hierarchy&lt;/p>"/>
                            <title_1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h4"
                                    size="is-6"
                                    text="&lt;p>4444 wrong order element&lt;/p>"/>
                            <title_2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h3"
                                    size="is-5"
                                    text="&lt;p>333 must be visible in ToC&lt;/p>"/>
                            <title_3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h2"
                                    size="is-4"
                                    text="&lt;p>22 with no children&lt;/p>"/>
                            <title_4
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h2"
                                    size="is-4"
                                    text="&lt;p>22 with correct hierarchy&lt;/p>"/>
                            <title_1_1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h3"
                                    size="is-5"
                                    text="&lt;p>333 subelement with no children&lt;/p>"/>
                            <title_1_1_1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h3"
                                    size="is-5"
                                    text="&lt;p>333 subelement with children&lt;/p>"/>
                            <title_1_2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h4"
                                    size="is-6"
                                    text="&lt;p>4444 &lt;/p>"/>
                            <title_1_2_1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h4"
                                    size="is-6"
                                    text="&lt;p>4444 &lt;/p>"/>
                            <title_1_1_1_1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h3"
                                    size="is-5"
                                    text="&lt;p>333 last element&lt;/p>"/>
                        </column4>
                    </columns1>
                </container>
            </section>
            <footerreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/footerreference"/>
        </pagecontainer>
    </jcr:content>
</jcr:root>
