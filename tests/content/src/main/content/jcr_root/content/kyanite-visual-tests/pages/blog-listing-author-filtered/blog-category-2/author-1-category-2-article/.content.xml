<?xml version="1.0" encoding="UTF-8"?>
<jcr:root   xmlns:jcr="http://www.jcp.org/jcr/1.0"
            xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
            xmlns:ws="http://ds.pl/websight"
            xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="ws:Page">
    <jcr:content
            jcr:primaryType="ws:PageContent"
            jcr:title="author-1 category-2 article"
            sling:resourceType="kyanite/blogs/components/blogarticlepage"
            ws:template="/libs/kyanite/blogs/templates/blogarticlepage"
            description="author-1 category-2 article"
            heroImage="/content/kyanite-visual-tests/assets/Windows XP.png/"
            heroImageAlt="hero-image-alt"
            navbarFixed="true"
            navbarFixedPosition="has-navbar-fixed-top"
            publicationDate="2000-01-10"
            readTime="5"
            title="author-1 category-2 article">
        <author
                jcr:primaryType="nt:unstructured"
                authorInfoSource="authorPage"
                authorPageLink="/content/kyanite-visual-tests/pages/blog-listing-author-filtered/author-1/"/>
        <pagecontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kyanite/common/components/pagecontainer">
            <navbarreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/navbarreference"/>
            <section
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/section">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kyanite/common/components/container">
                    <columns1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/columns"
                            columnsActivationLevel="is-desktop"
                            isMultiline="true">
                        <column1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="two"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <backbutton
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogpostbackbutton"/>
                        </column1>
                        <column2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogarticleheader
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogarticleheader">
                                <author
                                        jcr:primaryType="nt:unstructured"
                                        authorInfoSource="parentPage"/>
                            </blogarticleheader>
                        </column2>
                        <column3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogposttableofcontents
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogposttableofcontents"
                                    maxHeadingLevel="2"
                                    minHeadingLevel="6"
                                    showBackToTopButton="true"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                        </column3>
                        <column4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offset="two"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    element="h2"
                                    text="Title"/>
                            <content
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/content"
                                    color=""
                                    size="is-medium"
                                    text="&lt;p>Content&lt;/p>">
                            </content>
                        </column4>
                    </columns1>
                </container>
            </section>
            <footerreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/footerreference"/>
        </pagecontainer>
    </jcr:content>
</jcr:root>
