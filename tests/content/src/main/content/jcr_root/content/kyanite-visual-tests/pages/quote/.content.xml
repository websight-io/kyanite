<?xml version="1.0" encoding="UTF-8"?>
<jcr:root   xmlns:jcr="http://www.jcp.org/jcr/1.0"
            xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
            xmlns:ws="http://ds.pl/websight"
            xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="ws:Page">
    <jcr:content
            jcr:primaryType="ws:PageContent"
            jcr:title="Quote"
            sling:resourceType="kyanite/blogs/components/blogarticlepage"
            ws:template="/libs/kyanite/blogs/templates/blogarticlepage"
            navbarFixed="true"
            navbarFixedPosition="has-navbar-fixed-top"
            publicationDate="2024-05-20"
            readTime="5"
            title="Quote">
        <author
                jcr:primaryType="nt:unstructured"
                authorDescription="A passionate learner and dedicate writer"
                authorInfoSource="ownProperties"
                authorName="John Doe"
                authorPhoto="/content/kyanite-visual-tests/assets/account-circle.png"
                authorPhotoAlt="John Doe"
                authorRole="CEO"/>
        <pagecontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kyanite/common/components/pagecontainer">
            <navbarreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/navbarreference"/>
            <section
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/section/fixedsection"
                    renderAsHero="false">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kyanite/common/components/container/fixedcontainer">
                    <columns1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/columns/fixedcolumns"
                            columnsActivationLevel="is-desktop"
                            isMultiline="true">
                        <column1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="two"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <backbutton
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogpostbackbutton"/>
                        </column1>
                        <column2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogarticleheader
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogarticleheader">
                                <author
                                        jcr:primaryType="nt:unstructured"
                                        authorInfoSource="parentPage"/>
                            </blogarticleheader>
                        </column2>
                        <column3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column/fixedcolumn"
                                customClass="sticky-container">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <blogposttableofcontents
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogposttableofcontents"
                                    maxHeadingLevel="2"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                        </column3>
                        <column4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="kyanite/common/components/columns/column">
                            <desktopColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offset="two"
                                    offsetType="evenNumber"
                                    size="eight"
                                    sizeType="evenNumber"/>
                            <tabletColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <mobileColumnStyle
                                    jcr:primaryType="nt:unstructured"
                                    isNormalColumn="true"
                                    offsetType="evenNumber"
                                    sizeType="evenNumber"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/common/components/title"
                                    addAnimatedEndings="{Boolean}false"
                                    delay="{Long}1200"
                                    element="h2"
                                    isSpaced="{Boolean}false"
                                    showCursor="{Boolean}false"
                                    size="is-2"
                                    speed="{Long}50"
                                    subtitle="Subtitle"
                                    text="Title"/>
                            <quote_with_photo
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/quote"
                                    authorCompany="Dynamic Solutions"
                                    authorName="Jane Doe"
                                    authorPhotoAlt="Jane Doe"
                                    authorPhotoPath="/content/kyanite-visual-tests/assets/account-circle.png/"
                                    quoteText="&lt;p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Fusce consequat risus in tortor maximus interdum. Praesent quis risus lobortis, pulvinar dui et, varius augue. Proin non dolor vitae ante dictum maximus laoreet et mauris. Fusce quis tellus et nunc porttitor consequat. Nulla convallis dui odio, a dictum arcu feugiat eu.&lt;/p>"/>
                            <quote_no_photo
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/quote"
                                    authorCompany="Dynamic Solutions"
                                    authorName="Jane Doe"
                                    authorPhotoAlt="Jane Doe"
                                    authorPhotoPath="\0"
                                    quoteText="&lt;p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Fusce consequat risus in tortor maximus interdum. Praesent quis risus lobortis, pulvinar dui et, varius augue. Proin non dolor vitae ante dictum maximus laoreet et mauris. Fusce quis tellus et nunc porttitor consequat. Nulla convallis dui odio, a dictum arcu feugiat eu.&lt;/p>"/>
                            <authorBio
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="kyanite/blogs/components/blogarticleauthorbio">
                                <author
                                        jcr:primaryType="nt:unstructured"
                                        authorInfoSource="parentPage"/>
                            </authorBio>
                        </column4>
                    </columns1>
                </container>
            </section>
            <footerreference
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/footerreference"/>
        </pagecontainer>
    </jcr:content>
</jcr:root>
