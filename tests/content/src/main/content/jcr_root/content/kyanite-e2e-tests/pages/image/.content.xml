<?xml version="1.0" encoding="UTF-8"?>
<jcr:root   xmlns:jcr="http://www.jcp.org/jcr/1.0"
            xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
            xmlns:ws="http://ds.pl/websight"
            xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
        jcr:primaryType="ws:Page">
    <jcr:content
            jcr:primaryType="ws:PageContent"
            jcr:title="Image"
            sling:resourceType="kyanite/common/components/page"
            ws:template="/libs/kyanite/blogs/templates/articlepage"
            navbarFixed="true">
        <pagecontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="kyanite/common/components/pagecontainer">
            <navbar
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/navbarreference"/>
            <section
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/section" renderAsHero="false">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="kyanite/common/components/container">
                    <title_with_anchor
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/title"
                            anchorId="title_anchor"
                            element="h2"
                            text="Title with anchor"
                            type="title"/>
                    <title1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/title"
                            anchorId="title_anchor"
                            element="h4"
                            text="Open in a new tab"
                            type="title"/>
                    <image_open_external_page_in_new_tab
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/image"
                            alt="\0"
                            assetReference="/content/kyanite-e2e-tests/assets/images/image-thumbnail.svg"
                            hasVideoOptions="false"
                            height="300"
                            imageSrcType="link"
                            isCodeSvg="true"
                            isRounded="false"
                            openInNewTab="true"
                            style="is-160x160"
                            type="none"
                            url="https://google.com"
                            width="300"/>
                    <title2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/title"
                            anchorId="title_anchor"
                            element="h4"
                            text="Open in current tab"
                            type="title"/>
                    <image_open_external_page_in_current_tab
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/image"
                            alt="\0"
                            assetReference="/content/kyanite-e2e-tests/assets/images/image-thumbnail.svg"
                            hasVideoOptions="false"
                            height="300"
                            imageSrcType="asset"
                            isCodeSvg="true"
                            isRounded="false"
                            openInNewTab="false"
                            type="none"
                            url="https://google.com"
                            width="300"/>
                    <title3
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/title"
                            anchorId="title_anchor"
                            element="h4"
                            text="Open a WS page"
                            type="title"/>
                    <image_open_ws_page
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/image"
                            alt="\0"
                            assetReference="/content/kyanite-e2e-tests/assets/images/image-thumbnail.svg"
                            hasVideoOptions="false"
                            height="300"
                            imageSrcType="asset"
                            isCodeSvg="true"
                            isRounded="false"
                            openInNewTab="true"
                            style="is-160x160"
                            type="none"
                            url="/content/kyanite-e2e-tests/pages/section.html"
                            width="300"/>
                    <title4
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/title"
                            anchorId="title_anchor"
                            element="h4"
                            text="Go to anchor"
                            type="title"/>
                    <image_go_to_anchor
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="kyanite/common/components/image"
                            alt="\0"
                            assetReference="/content/kyanite-e2e-tests/assets/images/image-thumbnail.svg"
                            hasVideoOptions="false"
                            height="300"
                            imageSrcType="asset"
                            isCodeSvg="true"
                            isRounded="false"
                            openInNewTab="false"
                            type="none"
                            url="#title_anchor"
                            width="300"/>
                </container>
            </section>
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="kyanite/common/components/footerreference"/>
        </pagecontainer>
    </jcr:content>
</jcr:root>
