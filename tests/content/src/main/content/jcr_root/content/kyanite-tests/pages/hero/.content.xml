<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:ws="http://ds.pl/websight" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  jcr:primaryType="ws:Page">
  <jcr:content
    jcr:primaryType="ws:PageContent"
    jcr:title="Hero"
    sling:resourceType="kyanite/components/page"
    ws:template="kyanite/templates/basicpage">
    <pagecontainer
      jcr:primaryType="nt:unstructured"
      sling:resourceType="kyanite/components/pagecontainer">
      <hero
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/hero"/>
      <container
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-medium"
          subTitle="Warning"
          title="Medium"
          variant="is-warning"/>
      </container>
      <container_1
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-large"
          subTitle="Danger"
          title="Large"
          variant="is-danger"/>
      </container_1>
      <container_2
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-halfheight"
          subTitle="Info"
          title="Half-height"
          variant="is-info"/>
      </container_2>
      <container_3
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-small"
          subTitle="Primary"
          title="Small"
          variant="is-primary"/>
      </container_3>
      <container_3_1
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-small"
          subTitle="Success"
          title="Small"
          variant="is-success"/>
      </container_3_1>
      <container_3_1_1
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-small"
          subTitle="Light"
          title="Small"
          variant="is-light"/>
      </container_3_1_1>
      <container_3_1_2
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kyanite/components/container">
        <hero
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kyanite/components/hero"
          size="is-small"
          subTitle="Dark"
          title="Small"
          variant="is-dark"/>
      </container_3_1_2>
    </pagecontainer>
  </jcr:content>
</jcr:root>
