{
  "include": [
    "../src/main/webapp/**/*"
  ],
  "compilerOptions": {
    "baseUrl": "./kyanite-richtext",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "target": "es6",
    "jsx": "preserve",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  }
}
