          "sling:resourceType": "wcm/dialogs/components/radio/option",
          "label": "Links column",
          "selected": true,
          "value": "links"
        },
        "highlights": {
          "sling:resourceType": "wcm/dialogs/components/radio/option",
          "label": "Highlights column",
          "value": "highlights"
        }
      },
