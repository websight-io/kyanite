{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "generalTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "types": {
        "sling:resourceType": "wcm/dialogs/components/multifield",
        "name": "types",
        "label": "Types",
        "type": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "label",
          "label": "Type of inquiry",
          "description": "E-mail message subject."
        },
        "email": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "email",
          "label": "Email",
          "description": "Email for sending"
        }
      },
      "formHeader": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "formHeader",
        "label": "Form Header Text"
      },
      "showMessage": {
        "sling:resourceType": "wcm/dialogs/components/toggle",
        "name": "showMessage",
        "label": "Show Message field"
      },
      "messageContainer": {
        "sling:resourceType": "wcm/dialogs/components/container",
        "messageLabel": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "messagePlaceholder",
          "label": "Message placeholder"
        },
        "ws:display": {
          "condition": {
            "sourceName": "showMessage",
            "values": "true"
          }
        }
      },
      "submitLabel": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "submitLabel",
        "label": "Label of Submit action"
      },
      "dialogConsentText": {
        "sling:resourceType": "wcm/dialogs/components/richtext",
        "name": "consentText",
        "label": "Consent Text",
        "ws:disallowedContext": ["edit:panel"]
      },
      "panelConsentText": {
        "sling:resourceType": "wcm/dialogs/components/richtext",
        "name": "consentText",
        "label": "Consent Text",
        "ws:disallowedContext": ["edit:dialog"],
        "configuration": "wcm/dialogs/components/richtext/configurations/compact"
      }
    }
  }
}
