{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "generalTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "type": {
        "sling:resourceType": "wcm/dialogs/components/radio",
        "name": "type",
        "label": "Create container positioning on side",
        "required": true,
        "description": "Type of the navbar item",
        "link": {
          "sling:resourceType": "wcm/dialogs/components/radio/option",
          "label": "Link",
          "value": "link",
          "selected": true
        },
        "image": {
          "sling:resourceType": "wcm/dialogs/components/radio/option",
          "label": "Image",
          "value": "image"
        },
        "container": {
          "sling:resourceType": "wcm/dialogs/components/radio/option",
          "label": "Container",
          "value": "container"
        }
      },
      "linkContainer": {
        "sling:resourceType": "wcm/dialogs/components/container",
        "label": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "label",
          "label": "Item label"
        },
        "url": {
          "sling:resourceType": "wcm/dialogs/components/pathpicker",
          "rootPath": "/content",
          "name": "url",
          "label": "URL connected to item"
        },
        "addIcon": {
          "sling:resourceType": "wcm/dialogs/components/toggle",
          "name": "addIcon",
          "label": "Add icon?"
        },
        "iconContainer": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "iconLibType": {
            "sling:resourceType": "wcm/dialogs/components/select",
            "name": "iconLibType",
            "label": "Icon Library",
            "required": true,
            "datasource": {
              "sling:resourceType": "/apps/iconLibraryDatasource"
            }
          },
          "selectOrType": {
            "sling:resourceType": "wcm/dialogs/components/radio",
            "name": "selectOrType",
            "label": "Select icon or type it",
            "required": true,
            "select": {
              "sling:resourceType": "wcm/dialogs/components/radio/option",
              "label": "Select it",
              "value": "select",
              "selected": true
            },
            "type": {
              "sling:resourceType": "wcm/dialogs/components/radio/option",
              "label": "Type it",
              "value": "type"
            }
          },
          "typedIcon": {
            "sling:resourceType": "wcm/dialogs/components/textfield",
            "name": "icon",
            "label": "Name of the icon",
            "ws:display": {
              "condition": {
                "sourceName": "selectOrType",
                "values": "type"
              }
            }
          },
          "iconFA": {
            "sling:resourceType": "wcm/dialogs/components/container",
            "selectableIcon": {
              "sling:resourceType": "wcm/dialogs/components/include",
              "path": "kyanite/common/components/common/icon/icons/fontawesome",
              "ws:display": {
                "condition": {
                  "sourceName": "selectOrType",
                  "values": "select"
                }
              }
            },
            "iconSize": {
              "sling:resourceType": "wcm/dialogs/components/include",
              "path": "kyanite/common/components/common/icon/iconsize/fontawesome"
            },
            "ws:display": {
              "condition": {
                "sourceName": "iconLibType",
                "values": "fas"
              }
            }
          },
          "iconMD": {
            "sling:resourceType": "wcm/dialogs/components/container",
            "selectableIcon": {
              "sling:resourceType": "wcm/dialogs/components/include",
              "path": "kyanite/common/components/common/icon/icons/materialdesign",
              "ws:display": {
                "condition": {
                  "sourceName": "selectOrType",
                  "values": "select"
                }
              }
            },
            "iconSize": {
              "sling:resourceType": "wcm/dialogs/components/include",
              "path": "kyanite/common/components/common/icon/iconsize/materialdesign"
            },
            "ws:display": {
              "condition": {
                "sourceName": "iconLibType",
                "values": "mdi"
              }
            }
          },
          "ws:display": {
            "condition": {
              "sourceName": "addIcon",
              "values": "true"
            }
          }
        },
        "ws:display": {"condition": {
          "sourceName": "type",
          "values": "link"
        }}
      },
      "imageContainer": {
        "sling:resourceType": "wcm/dialogs/components/container",
        "imageSource": {
          "sling:resourceType": "wcm/dialogs/components/include",
          "include": {
            "sling:resourceSuperType": "kyanite/common/components/common/imagesource",
            "assetReference": {
              "mimeTypes": ["image/*"],
              "name": "image/assetReference"
            },
            "src": {
              "name": "image/assetReference"
            }
          }
        },
        "alt": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "image/alt",
          "label": "Image alt"
        },
        "isLink": {
          "sling:resourceType": "wcm/dialogs/components/toggle",
          "name": "isLink",
          "label": "Is link"
        },
        "linkContainer": {
          "sling:resourceType": "wcm/dialogs/components/container",
          "url": {
            "sling:resourceType": "wcm/dialogs/components/pathpicker",
            "rootPath": "/content",
            "name": "imageUrl",
            "label": "URL"
          },
          "openInNewTab": {
            "sling:resourceType": "wcm/dialogs/components/toggle",
            "name": "openInNewTab",
            "label": "Open link in a new tab"
          },
          "ws:display": {
            "condition": {
              "sourceName": "isLink",
              "values": "true"
            }
          }
        },
        "width": {
          "sling:resourceType": "wcm/dialogs/components/numberfield",
          "name": "imageWidth",
          "label": "Image width (px)"
        },
        "height": {
          "sling:resourceType": "wcm/dialogs/components/numberfield",
          "name": "imageHeight",
          "label": "Image height (px)"
        },
        "ws:display": {"condition": {
          "sourceName": "type",
          "values": "image"
        }}
      }
    }
  }
}