{
  "jcr:primaryType": "ws:Page",
  "jcr:content": {
    "jcr:primaryType": "ws:PageContent",
    "sling:resourceType": "kyanite/blogs/components/blogarticlepage",
    "author": {
      "jcr:primaryType": "nt:unstructured",
      "authorInfoSource": "authorPage",
      "socialMedia": {
        "jcr:primaryType": "nt:unstructured",
        "sling:resourceType": "kyanite/blogs/components/blogarticleauthorbio/socialmedia"
      }
    },
    "navbarFixed": "true",
    "navbarFixedPosition": "has-navbar-fixed-top",
    "pagecontainer": {
      "sling:resourceType": "kyanite/common/components/pagecontainer",
      "navbarreference": {
        "sling:resourceType": "kyanite/common/components/navbarreference"
      },
      "section": {
        "sling:resourceType": "kyanite/common/components/section/fixedsection",
        "renderAsHero": "false",
        "container": {
          "sling:resourceType": "kyanite/common/components/container/fixedcontainer",
          "columns1": {
            "sling:resourceType": "kyanite/common/components/columns/fixedcolumns",
            "columnsActivationLevel": "is-desktop",
            "isMultiline": "true",
            "column1": {
              "sling:resourceType": "kyanite/common/components/columns/column/fixedcolumn",
              "customClass": "sticky-container",
              "desktopColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "size": "two",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "tabletColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "mobileColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "backbutton": {
                "sling:resourceType": "kyanite/blogs/components/blogpostbackbutton"
              }
            },
            "column2": {
              "sling:resourceType": "kyanite/common/components/columns/column/fixedcolumn",
              "desktopColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "size": "eight",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "tabletColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "mobileColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "blogarticleheader": {
                "sling:resourceType": "kyanite/blogs/components/blogarticleheader",
                "author": {
                  "authorInfoSource": "parentPage"
                }
              }
            },
            "column3": {
              "sling:resourceType": "kyanite/common/components/columns/column/fixedcolumn",
              "customClass": "sticky-container",
              "desktopColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "size": "two"
              },
              "blogposttableofcontents": {
                "sling:resourceType": "kyanite/blogs/components/blogposttableofcontents",
                "maxHeadingLevel": "2",
                "minHeadingLevel": "6",
                "showBackToTopButton": true
              },
              "desktopColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "tabletColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "mobileColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              }
            },
            "column4": {
              "sling:resourceType": "kyanite/common/components/columns/column",
              "customClass" : "previewImageContainer",
              "desktopColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "size": "eight",
                "offset": "two",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "tabletColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "mobileColumnStyle": {
                "jcr:primaryType": "nt:unstructured",
                "sizeType": "evenNumber",
                "offsetType": "evenNumber",
                "isNormalColumn": "true"
              },
              "title": {
                "sling:resourceType": "kyanite/common/components/title",
                "text": "Title",
                "element": "h2"
              },
              "content": {
                "sling:resourceType": "kyanite/common/components/content",
                "text": "<p>Content</p>",
                "size": "is-medium"
              },
              "authorBio": {
                "sling:resourceType": "kyanite/blogs/components/blogarticleauthorbio",
                "author": {
                  "jcr:primaryType": "nt:unstructured",
                  "authorInfoSource": "parentPage"
                }
              }
            }
          }
        }
      },
      "footerreference": {
        "sling:resourceType": "kyanite/common/components/footerreference"
      }
    }
  }
}
